exports[`db2 export table command should fail with empty table name 1`] = `
"
Syntax Error:
Missing Positional Argument: table
Argument Description: The name of the table to export

Example:

 - Export employees data from the table SAMPLE.EMP and save it to the file
 'employees.sql':

      $ zowe db2 export table SAMPLE.EMP --outfile employees.sql

Use \\"zowe db2 export table --help\\" to view command description, usage, and options.
"
`;

exports[`db2 export table command should fail with invalid option 1`] = `
"Command Error:
Unknown argument: format
Command failed due to improper syntax
Command entered: \\"zowe db2 export table SYSIBM.SYSDUMMY1 --format json\\"
Available commands are \\"table\\".
Use \\"zowe db2 export --help\\" to view groups, commands, and options.
Error Details:
Unknown argument: format
"
`;
